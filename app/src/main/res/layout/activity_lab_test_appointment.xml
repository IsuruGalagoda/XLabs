<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/back1"    tools:context=".LabTestAppointment">    <TextView        android:id="@+id/txtLabApp"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Lab Test Appointment"        android:textColor="#000000"        android:textSize="28sp"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.05" />    <EditText        android:id="@+id/etxtPname"        android:layout_width="385dp"        android:layout_height="30dp"        android:background="@drawable/g3"        android:ems="10"        android:hint=" Enter Patient Name..."        android:inputType="textPersonName"        android:textColor="#FFFFFF"        android:textColorHint="#FFFFFF"        android:textSize="18sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.615"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.333" />    <ImageView        android:id="@+id/imageView2"        android:layout_width="166dp"        android:layout_height="121dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.156"        app:srcCompat="@drawable/rr" />    <EditText        android:id="@+id/etxtEmailAddress"        android:layout_width="385dp"        android:layout_height="30dp"        android:background="@drawable/g3"        android:ems="10"        android:hint="Enter E-mail(abc@gmail.com)"        android:inputType="textEmailAddress"        android:textColor="#FFFFFF"        android:textColorHint="#FFFFFF"        android:textSize="18sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.692"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.696" />    <Button        android:id="@+id/btnSbAppoinmnt"        android:layout_width="200sp"        android:layout_height="40sp"        android:background="@drawable/g4"        android:text="Submit Appoinment"        android:textColor="@color/design_default_color_background"        android:textSize="18sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.497"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.916" />    <EditText        android:id="@+id/edtTextLabChemId"        android:layout_width="385dp"        android:layout_height="30dp"        android:layout_marginTop="16dp"        android:background="@drawable/g3"        android:ems="10"        android:hint=" Enter Selected Test No"        android:inputType="number"        android:textColor="#FFFFFF"        android:textColorHint="#FFFFFF"        android:textSize="18sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.615"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/etxtPname" />    <EditText        android:id="@+id/editTextPhone"        android:layout_width="385dp"        android:layout_height="30dp"        android:background="@drawable/g3"        android:ems="10"        android:hint="Enter Contact Number"        android:inputType="phone"        android:textColor="#FFFFFF"        android:textColorHint="#FFFFFF"        android:textSize="18sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.692"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.797" />    <Button        android:id="@+id/btnDatePicker"        android:layout_width="130dp"        android:layout_height="35dp"        android:layout_marginTop="20dp"        android:background="@drawable/custon_button2"        android:gravity="center_horizontal"        android:text="Appoinment Date"        android:textAlignment="center"        android:textColor="#070707"        android:textSize="15sp"        android:textStyle="bold"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.943"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/edtTextLabChemId" />    <Button        android:id="@+id/btnTimePicker"        android:layout_width="130dp"        android:layout_height="35dp"        android:background="@drawable/custon_button2"        android:text="Select Time"        android:textColor="#070707"        android:textSize="16sp"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.943"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.58" />    <EditText        android:id="@+id/edtextdatePicker"        android:layout_width="222dp"        android:layout_height="30dp"        android:layout_marginTop="20dp"        android:background="@drawable/g3"        android:ems="10"        android:hint=" Select a Date"        android:inputType="textPersonName"        android:textColor="#FFFFFF"        android:textColorHint="#FFFFFF"        android:textSize="18sp"        app:layout_constraintEnd_toStartOf="@+id/btnDatePicker"        app:layout_constraintHorizontal_bias="0.39"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/edtTextLabChemId" />    <EditText        android:id="@+id/edttxtTimePicker"        android:layout_width="222dp"        android:layout_height="30dp"        android:layout_marginStart="16dp"        android:layout_marginLeft="16dp"        android:background="@drawable/g3"        android:ems="10"        android:hint=" Select Time"        android:inputType="textPersonName"        android:textColor="#FFFFFF"        android:textColorHint="#FFFFFF"        android:textSize="18sp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintVertical_bias="0.583" /></androidx.constraintlayout.widget.ConstraintLayout>